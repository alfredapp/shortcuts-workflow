				<string>const allShortcuts = Application("Shortcuts Events").shortcuts()

const sfItems = allShortcuts.map(shortcut =&gt; {
  const name = shortcut.name()
  const folder = shortcut.folder.exists() ? shortcut.folder.name() : ""
  const inputMod = shortcut.acceptsInput() ?
    { subtitle: "Run with argument", valid: true } :
    { subtitle: "Shortcut does not accept input", valid: false }

  return {
    variables: { shortcut: name },
    uid: name + " " + folder,
    title: name,
    subtitle: folder,
