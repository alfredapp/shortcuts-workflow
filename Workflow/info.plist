				<string>// Helpers
function envVar(varName) {
  return $.NSProcessInfo.processInfo.environment.objectForKey(varName).js
}

function makeFolder(path) {
  $.NSFileManager
    .defaultManager
    .createDirectoryAtPathWithIntermediateDirectoriesAttributesError(
      path,
      true, undefined, undefined
    )
}

function readFile(path) {
  const data = $.NSFileManager.defaultManager.contentsAtPath(path)
  const string = $.NSString.alloc.initWithDataEncoding(data, $.NSUTF8StringEncoding).js
  return string
}

function writeFile(path, text) {
  $.NSString
    .alloc
    .initWithUTF8String(text)
    .writeToFileAtomicallyEncodingError(path, true, $.NSUTF8StringEncoding, undefined)
}

// Get STDOUT from command, as array split from newlines, without empty elements
function shortcutsCommand(...arguments) {
  const task = $.NSTask.alloc.init
  const stdout = $.NSPipe.pipe

  task.executableURL = $.NSURL.alloc.initFileURLWithPath("/usr/bin/shortcuts")
  task.arguments = arguments
  task.standardOutput = stdout
  task.launchAndReturnError(false)

  const dataOut = stdout.fileHandleForReading.readDataToEndOfFile
  const stringOut = $.NSString.alloc.initWithDataEncoding(dataOut, $.NSUTF8StringEncoding).js

  return stringOut.split("\n").filter(line =&gt; line)
}

// Cache Helpers
const cacheMinutes = 5
const cachePath = `${envVar("alfred_workflow_cache")}/cache.json`

function cacheExist() {
  return $.NSFileManager.defaultManager.fileExistsAtPath(cachePath)
}

function oldCache(ttlMinutes) {
  const ttl = ttlMinutes * 60 * 1000 // Milliseconds
  const currentTime = new Date().getTime()
  const cacheTime = parseInt(envVar("cache_age"))

  if (ttl + cacheTime &gt; currentTime) return false
  return true
}

function buildCache() {
// Array of arrays, listing shortcuts and their directories
  const shortcutsList = (function() {
    let list = []
    let uncategorised = shortcutsCommand("list")
    const folders = shortcutsCommand("list", "--folders")

    // Add shortcuts with folders to final list
    folders
      .forEach(folder =&gt; { // For each folder
        shortcutsCommand("list", "--folder-name", folder)
          .forEach(shortcut =&gt; { // For each shortcut in folder
            uncategorised.splice(uncategorised.indexOf(shortcut), 1) // Remove from uncategorised
            list.push([shortcut, folder])
          })
      })

    // Add folderless shortcuts to final list
    uncategorised.forEach(shortcut =&gt; list.push([shortcut, ""]))

    return list
  })()

  // Build items
  const sfItems = shortcutsList.map(categorised =&gt; {
    const shortcut = categorised[0]
    const folder = categorised[1]

    const inputType = (function() {
      // If input paths have been specificed
      const envInputPaths = envVar("input_paths")

      if (envInputPaths) {
        const inputPaths = envInputPaths.split("\t")
        const pathsString = inputPaths.join("\n")

        return {
          subtitle: inputPaths.length &gt; 1 ? "Paths: " + pathsString : "Path: " + pathsString,
          mods: { cmd: { valid: false, subtitle: "Input already set: " + pathsString } }
        }
      }

      // If an argument has been specified
      const envArgument = envVar("argument")

      if (envArgument) return {
        subtitle: "Argument: " + envArgument,
        mods: { cmd: { valid: false, subtitle: "Input already set: " + envArgument } }
      }

      // Else return folder name and allow new arguments
      return {
        subtitle: folder,
        mods: { cmd: { subtitle: "Run with argument" } }
      }
    })()

    return {
      variables: { shortcut: shortcut },
      uid: shortcut + " " + folder,
      title: shortcut,
      subtitle: inputType.subtitle,
      mods: inputType.mods,
      match: shortcut + " " + folder
    }
  })

  // Build JSONs
  const nowJSON = JSON.stringify({ items: sfItems })

  const cacheJSON = JSON.stringify({
    variables: { cache_age: new Date().getTime() },
    rerun: 0.1,
    items: sfItems
  })

  // Write cache
  makeFolder(envVar("alfred_workflow_cache"))
  writeFile(cachePath, cacheJSON)

  // Output result
  return nowJSON
}

// Main
function run() {
  // No cache, build it
  if (!cacheExist()) return buildCache()

  // Cache never loaded, load from disk for instant results
  if (!envVar("cache_age")) return readFile(cachePath)

  // Old cache, rebuild
  if (oldCache(cacheMinutes)) return buildCache()

  // Recent cache, load from disk
  return readFile(cachePath)
}</string>
