				<key>script</key>
				<string>require 'json'
require 'open3'

Folders = Open3.capture2('/usr/bin/shortcuts', 'list', '--folders').first.split("\n")

Script_filter_items = Folders.each_with_object([]) { |folder, array|
  array.push(
    uid: folder,
    title: folder,
    arg: "#{folder} "
  )
}

puts({ items: Script_filter_items }.to_json)</string>
