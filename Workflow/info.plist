				<string>const allShortcuts = Application("Shortcuts Events").shortcuts()

const sfItems = allShortcuts.map(shortcut =&gt; {
  if (!shortcut.acceptsInput()) return null

  const name = shortcut.name()
  const folder = shortcut.folder.exists() ? shortcut.folder.name() : "[No folder]"

  return {
    variables: { shortcut: name },
    uid: name + " " + folder,
    title: name,
    subtitle: folder,
    match: name + " " + folder
  }
}).filter(shortcut =&gt; shortcut)

// Output JSON
JSON.stringify({ cache: { seconds: 300, loosereload: true }, items: sfItems })</string>
